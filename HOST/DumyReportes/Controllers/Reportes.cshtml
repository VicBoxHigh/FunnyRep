@model IEnumerable<DumyReportes.Models.Report>

@{
    ViewBag.Title = "Reportes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Reportes</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ValidateResult)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdUserWhoNotified)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DTCreation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ValidateResult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdUserWhoNotified)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DTCreation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdReport }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdReport }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdReport })
        </td>
    </tr>
}

</table>
